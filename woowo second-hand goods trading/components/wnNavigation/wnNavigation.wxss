.wn-nav-container {
  position: fixed;
  z-index: 9999;
  left: 0;
  right: 0;
  /* background-color: rgb(246, 182, 45); */
}

.wn-navigator {
  position: relative;
}

.nav-title {
  width: 100%;
  height: 100%;
  position: absolute; 
  left: 0;
  right: 0;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 88rpx;
  text-align: center;
  color: rgb(44, 62, 80);
  z-index: 9;
}

.nav-function {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  right: 0;
  z-index: 99;
}

.nav-fnt-back {
  box-sizing: content-box;
  width: 40rpx;
  height: 40rpx;
  /* background-color: red; */
  padding: 24rpx 20rpx;
  display: flex;
}

.nav-back-icon {
  width: 20rpx;
  height: 40rpx;
  display: block;
  margin: auto;
}

.normal-hover {
  opacity: 0.7;
} 