.nothing-mark {
  width: 286rpx;
  margin: 0 auto;
}
.nothing-mark-img {
  width: 100%;
  height: 232rpx;
}
.nothing-mark-txt {
  display: block;
  width: 100%;
  height: 42rpx;
  line-height: 42rpx;
  color: rgb(44, 62, 80);
  font-size: 30rpx;
  text-align: center;
  margin-top: 20rpx;
}

.mini-goods-item {
  width: 100%;
  background-color: #fff;
  overflow: hidden;
}
.mini-goods-cover {
  float: left;
  width: 180rpx;
  height: 150rpx;
  background-color: rgb(44, 62, 80);
  margin: 0 30rpx 0 20rpx;
}
.mini-goods-inf {
  float: left;
}

.mini-goods-title,
.mini-goods-price {
  display: block;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  color: rgb(44, 62, 80);
  margin: 20rpx 0;
}
.mini-goods-price {
  color: rgb(246, 182, 45);
}