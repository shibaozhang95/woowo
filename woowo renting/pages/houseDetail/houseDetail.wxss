page {
  background-color: rgb(248, 245, 240);
}

.container {
  position: relative;
  padding-bottom: 40rpx;
  overflow: hidden;
}

.images-block {
  width: 100%;
  height: 600rpx;
  background-color: rgb(53, 89, 110);
}

.images-block swiper,
.images-block swiper-item,
.images-block .house-images {
  width: 100%;
  height: 100%;
}

.price-and-location-block {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx 20rpx 30rpx;
  background-color: rgb(255, 255, 255);
  margin-bottom: 30rpx;
  overflow: hidden;
}

.price-and-count-block {
  width: 100%;
  /* height: 84rpx; */
  margin: 10rpx 0 0 0;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: flex-end;
}

.price-block {
  height: 84rpx;
  line-height: 84rpx;
  font-size: 60rpx;
  color: rgb(246, 182, 45);
}

.count-block {
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-end;
}

.view-count, .liked-count {
  height: 34rpx;
  font-size: 24rpx;
  color: rgba(44, 62, 80, 0.5);
  margin-left: 30rpx;
}

.house-title-block {
  line-height: 56rpx;
  font-size: 40rpx;
  color: rgb(44, 62, 80);
  margin: 10rpx 0 0 0;
  width: 690rpx;
  white-space: normal;
}

.location-block {
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: flex-start;
  margin: 10rpx 0 0 0;
}

.location-text {
  width: 560rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  color: rgba(44, 62, 80, 0.5);
}

.location-icon-block {
  width: 100rpx;
  height: 100rpx;
  position: relative;
}

.location-icon {
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.location-icon-text {
  width: 100rpx;
  height: 28rpx;
  font-size: 20rpx;
  line-height: 28rpx;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 6rpx;
  color: rgba(44, 62, 80, 0.8);
}

.post-time {
  margin: 10rpx 0 0 0;

  height: 34rpx;
  line-height: 34rpx;
  font-size: 24rpx;
  color: rgba(44, 62, 80, 0.5);
}

.basic-house-info-block {
  width: 100%;
  box-sizing: border-box;
  padding: 0 30rpx;
  height: 250rpx;
  background-color: rgb(255, 255, 255);
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}

.basic-one-block {
  width: 230rpx;
  height: 150rpx;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
}

.basic-info-icon {
  width: 100rpx;
  height: 70rpx;
  display: block;
  margin-bottom: 12rpx;
}
.basic-info-line-one {
  /* width: 100%; */
  height: 34rpx;
  line-height: 34rpx;
  /* text-align: center; */
  font-size: 24rpx;
  color: rgb(44, 62, 80);
}
.basic-info-line-two {
  /* width: 100%; */
  height: 34rpx;
  line-height: 34rpx;
  /* text-align: center; */
  font-size: 24rpx;
  color: rgba(44, 62, 80, 0.5);
}

.house-holder-block {
  margin: 2rpx;
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  height: 214rpx;
  background-color: rgb(255, 255, 255);
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: center;
}

.house-holder-avatar {
  width: 100rpx;
  height: 100rpx;
  background-color: rgb(136, 193, 211);
  border-radius: 50%;
}

.username-and-right-block {
  width: 380rpx;
  margin: 0 30rpx;
}
.username-and-right-block .username {
  width: 100%;
  height: 56rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  line-height: 56rpx;
  font-size: 40rpx;
  color: rgb(44, 62, 80);
}
.username-and-right-block .user-right {
  width: 100%;
  height: 40rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  line-height: 40rpx;
  font-size: 28rpx;
  color: rgba(44, 62, 80, 0.5);
}

.house-holder-fnt-block {
  width: 150rpx
}

.house-holder-fnt-block .fnt {
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  color: rgb(255, 255, 255);
  font-size: 28rpx;
  border-radius: 20rpx;
}

.fnt-message {
  background-color: rgb(246, 182, 45);
  margin-bottom: 10rpx;
}
.fnt-follow-btn {
  width: 150rpx;
  height: 50rpx;
  box-sizing: border-box;
  line-height: 50rpx;
  font-size: 28rpx;
  border-radius: 20rpx;
} 

.fnt-following {
  border: 2rpx solid rgba(44, 62, 90, 0.5);
  background-color: rgba(44, 62, 80, 0);
  color: rgba(44, 62, 80, 0.5);
}
.fnt-unfollowing {
  border: 2rpx solid rgb(44,62, 90);
  background-color: rgb(44, 62, 80);
  color: rgb(255, 255, 255);
}

/*
*  Facilities block
*/

.facilities-block {
  width: 100%;
  background-color: rgb(255, 255, 255);
  overflow: hidden;
  margin-bottom: 2rpx;
}

.facilities-title-block {
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: flex-end;
  margin: 40rpx 0;
}


.facilities-title {
  height: 44rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: rgb(44, 62, 80);
}

.facilities-more-block {
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-end;
  align-items: center;
}

.facilities-more {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: rgb(246, 182, 45);
}

.facilities-more-icon {
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
  margin-left: 10rpx;
  display: flex;
  flex-flow: row nowrap;
}

.facilities-icons-block {
  width: 100%;
  height: 110rpx;
  padding-left: 40rpx;
  margin: 40rpx 0;
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
}

.one-icon-block {
  position: relative;
  margin-left: -40rpx;
}
.one-icon-image {
  width: 180rpx;
  height: 110rpx;
}
.icon-text {
  width: 180rpx;
  height: 28rpx;
  line-height: 28rpx;
  font-size: 20rpx;
  color: rgb(44, 62, 80);
  position: absolute;
  left: 0;
  bottom: 6rpx;
  text-align: center;
}
.icon-more {
  width: 50rpx;
  height: 10rpx;
  margin-left: 30rpx;
}

.house-detail-block {
  width: 100%;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  background-color: rgb(255, 255, 255);
  margin-bottom: 2rpx;
}

.house-detail-title {
  height: 44rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: rgb(44, 62, 80);
  margin-bottom: 20rpx;
}

.house-detail {
  font-size: 32rpx;
  line-height: 44rpx;
  color: rgb(44, 62, 80);
  white-space: pre-wrap;
}

.house-rules-container {
  width: 100%;
  background-color: rgb(255, 255, 255);
  overflow: hidden;
}

.house-rules-block {
  width: 100%;
  box-sizing: border-box;
  padding: 40rpx 30rpx;
  border-radius: 10rpx;
}

.house-rules-title {
  height: 44rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  color: rgb(44, 62, 80);
  font-weight: bold;
  margin-bottom: 20rpx;
}

.house-rules-gender {
  height: 44rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  color: rgb(44, 62, 80);
  margin-bottom: 20rpx;
}

.house-rules-if-allowed {
  display: flex;
  flex-flow: row wrap;
  margin-left: -20rpx;
  margin-bottom: 20rpx;
}

.if-allowed-item {
  height: 44rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: center;
  margin-left: 20rpx;
}

.if-allowed-icon {
  width: 30rpx;
  height: 30rpx;
  border-radius: 50%;
  
  margin-right: 10rpx;
}

.if-allowed-text {
  height: 44rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  color: rgb(44, 62, 80);
}

.house-rules-others {
  font-size: 32rpx;
  line-height: 42rpx;
  color: rgb(44, 62, 80);
  white-space: pre-wrap;
}

.function-bar-block {
  position: fixed;
  z-index: 999;
  bottom: 0;
  left: 0;
  width: 100%;
  height: auto;
}

.normal-hover {
  opacity: 0.7;
}

.contact-show-icon image {
  width: 20rpx;
  height: 20rpx;
}

.show-less-icon image {
  width: 20rpx;
  height: 20rpx;
  transform: rotate(180deg);
}

.facilities-more-icon image {
  width: 20rpx;
  height: 20rpx;
  transform: rotate(270deg);
}