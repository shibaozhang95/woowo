page {
  background-color: rgb(248, 245, 240);
  overflow: hidden;
}

.container {
  overflow: hidden;
}

.success-block {
  width: 100%;
  /* height: 300rpx; */
  margin: 80rpx 0 50rpx 0;
  position: relative;
  display: flex;
  flex-flow: column nowrap;
  justify-content: center;
  align-items: center;
}

.success-block-tips {
  width: 100%;
  font-size: 50rpx;
  line-height: 72rpx;
  height: 72rpx;
  text-align: center;
  color: rgb(44, 62, 80);
  /* margin-bottom: 50rpx; */
}
