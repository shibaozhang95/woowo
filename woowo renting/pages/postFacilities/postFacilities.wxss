page {
  animation-name: appear;
  animation-duration: 2s;
  animation-iteration-count: 1;

  background-color: rgb(248, 245, 240);
}

@keyframes appear {
  from {opacity: 0;}
  to {opacity: 1;}
}

.container {
  width: 100%;
  background-color: rgb(248, 245, 240);
}

.f-section {
  width: 100%;
  background-color: rgb(255, 255, 255);
  border-top: 2rpx solid rgba(44, 62, 80, 0.1);
  margin-bottom: 40rpx;
}

.f-section .section-title {
  width: 100%;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  font-size: 32rpx;
  color: rgb(44, 62, 80);
  margin-top: 40rpx;
}

