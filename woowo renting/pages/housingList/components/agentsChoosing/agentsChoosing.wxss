.agents-choosing-container {
  position: fixed;
  background-color: rgba(44, 62, 80, 0.5);
  z-index: 99999;
}

.agents-list {
  position: absolute;
  top: 0;
  right: -600rpx;
  width: 600rpx;
  height: 100%;
  background-color: rgb(255, 255, 255);
}

.nothing-mark-block {
  width: 100%;
  height: auto;
  margin: 160rpx 0;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
}

.nothing-mark-block {
  width: 100%;
  height: auto;
  margin: 160rpx 0;
  display: flex;
  flex-flow: column nowrap;
  align-items: center;
}

.loading-block {
  width: 100%;
  height: auto;
  margin: 40rpx 0;
  display: flex;
  flex-flow: row nowrap;
  justify-content: center;
}

.loading-img {
  width: 40rpx;
  height: 40rpx;
  margin-right: 30rpx;
  display: block;
}

.loading-tips {
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: rgba(44, 62, 80, 0.5);
}

.one-agent {
  width: 100%;
  height: 160rpx;
  border-bottom: 2rpx solid rgba(44, 62, 80, 0.1);
  padding: 20rpx 40rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: flex-start;
  align-items: center;
  box-sizing: border-box;
}

.agent-logo {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
  margin-right: 20rpx;
  background-color: rgb(86, 182, 213);
}

.agent-name {
  width: 380rpx;
  height: 38rpx;
  line-height: 38rpx;
  font-size: 36rpx;
  color: rgb(44, 62, 80);
}

.normal-hover {
  opacity: 0.7;
}