page {
  width: 750rpx;
  box-sizing: border-box;
  background-color: rgb(255, 255, 255);
  overflow-x: hidden;
}

.container {
  width: 750rpx;
  background-color: rgb(255, 255, 255);
  padding-bottom: 40rpx;
  overflow-x: hidden;
}

.section {
  width: 750rpx;
  /* overflow: hidden; */
  /* overflow-x: hidden; */
  margin-bottom: 40rpx;
  position: relative;
}

.section-ad-and-search {
  width: 100%;
  height: 450rpx;
}

.section-ad-and-search search-bar {
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: 9;
}
