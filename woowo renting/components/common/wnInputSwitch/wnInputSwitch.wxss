.input-switch-container {
  width: 100%;
  height: 100rpx;
  box-sizing: border-box;
  padding: 0 40rpx;
  background-color: rgb(255, 255, 255);
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  align-items: center;
}

.input-title {
  height: 44rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  color: rgba(44, 62, 80, 0.8);
}
